<h1 id="notice"><%= @todoList.name%></h1>


<ul>
  <% @todoList.each do |u|%>
   <p><%= link_to u.title, user_todolist_path(u) %>  </p>
    <p><%= link_to u.description, user_todolist_path(u) %>  </p>
    <p><%= link_to u.category, user_todolist_path(u) %>  </p>
    <p><%= link_to u.status, user_todolist_path(u) %>  </p>
    <%= link_to 'Edit', edit_user_todolist_path(u) %> <%= link_to "Del", user_todolist_path(u), method: :delete%>
  <% end %>
</ul>

